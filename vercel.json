{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
          },
          {
            "source": "/(.*)",
            "destination": "/"
          }
    ],
    "version": 2,
    "buildCommand": "next build",
    "devCommand": "next dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "outputDirectory": ".next"
}